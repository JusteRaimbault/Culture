	Issn = {1460-7425},
	Journal = {Journal of Artificial Societies and Social Simulation},
	Keywords = {Agent-Based Simulations, Carrying Capacity, Pedestrian Dynamics, Evacuation Dynamics},
	Number = {1},
	Pages = {16},
	Reading = {A},
	Title = {Agent-Based Simulation of Pedestrian Behaviour in Closed Spaces: A Museum Case Study},
