	Publisher = {Public Library of Science},
	Title = {Why Does Public Transport Not Arrive on Time? The Pervasiveness of Equal Headway Instability},
	Url = {http://dx.doi.org/10.1371%2Fjournal.pone.0007292},
	Volume = {4},
	Year = {2009},
